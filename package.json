{
  "name": "vscode-insert-text",
  "version": "1.0.0",
  "description": "Easily insert text into the active editor",
  "license": "MIT",
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "tsc --watch --pretty --project ./",
    "lint": "tslint --project ./",
    "test": "npm run lint"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-vscode-insert-text.git"
  },
  "keywords": [
    "vscode",
    "vscode extension"
  ],
  "devDependencies": {
    "@types/node": "^12.12.3",
    "husky": "^3.0.9",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
